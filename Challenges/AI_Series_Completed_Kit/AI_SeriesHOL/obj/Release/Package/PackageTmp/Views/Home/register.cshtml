        <script type="text/javascript">

            $(document).ready(function () {
                $('#local_img').hide();
                $('#open_image_trigger').hide();
                $('#warn_msg').hide();
                document.getElementById("form_submit").disabled = true;


                navigator.getUserMedia = (navigator.getUserMedia ||
                    navigator.webkitGetUserMedia ||
                    navigator.mozGetUserMedia ||
                    navigator.msGetUserMedia);

                var video;
                var webcamStream; 1

                if (navigator.getUserMedia) {
                    navigator.getUserMedia(

                        {
                            video: true,
                            audio: false
                        },
                        function (localMediaStream) {
                            video = document.querySelector('video');

                            if (navigator.mediaDevices && navigator.mediaDevices.getUserMedia) {
                                navigator.mediaDevices.getUserMedia({ video: true }).then(function (stream) {

                                    video.srcObject = stream;
                                    video.play();
                                });

                            }
                        },
                        function (err) {
                            console.log("The following error occured: " + err);
                        }
                    );
                }
                else {
                    console.log("getUserMedia not supported");
                }

                var canvas, ctx;
                function init() {
                    canvas = document.getElementById("myCanvas");
                    ctx = canvas.getContext('2d');
                    var imageObj = new Image();

                    imageObj.onload = function () {
                        ctx.drawImage(imageObj, 0, 0, 150, 150);
                    };
                    imageObj.src = 'data:image/png;base64,';


                }


                $("#pagerefresh").click(function () {
                    $('#alert_popup').modal('hide');
                });


                var videoId = 'video';
                var scaleFactor = 1;
                var snapshots = [];

                function capture(video, scaleFactor) {
                    if (scaleFactor == null) {
                        scaleFactor = 1;
                    }
                    var w = video.videoWidth * scaleFactor;
                    var h = video.videoHeight * scaleFactor;
                    var canvas = document.createElement('canvas');
                    canvas.width = w;
                    canvas.height = h;
                    var ctx = canvas.getContext('2d');

                    ctx.drawImage(video, 0, 0, w, h);
                    return canvas;
                }


                $("#snap_btn").click(function (e) {
                    if ($('#check_id').is(":checked")) {
                        var check = "Yes";
                    }
                    else {
                        var check = "No";
                    }
                    var snap_btn_html = $('#snap_btn').html();

                    if (snap_btn_html == "Submit") {
                        e.preventDefault();
                        $('#snap_btn').button('loading');
                        var resultimg = img_src_64;
                        var img_64_array = resultimg.split(',');
                        
                        var image = JSON.stringify({ data: img_64_array[1], check: check });

                        console.log(check);

                        $.ajax({
                            type: "POST",
                            contentType: "application/json",
                            datatype: "json",
                            data: image,
                            url: "/Home/ImageValidationAPI/",
                            success: function (data) {
                                console.log(data);
                                if (data.Result == "Success") {
                                    $("#result_img").attr('src', resultimg);
                                    document.getElementById("form_submit").disabled = false;
                                }
                                else {

                                    console.log(data.Result);
                                    $("#errmsg").html(data.Result);
                                    $('#alert_popup').modal('show');
                                }
                                $('#snap_btn').button('reset');
                            }
                        });

                        snapshots.unshift(canvas);

                    }
                    else {
                        e.preventDefault();
                        $('#snap_btn').button('loading');
                        var canvas = capture(video, scaleFactor);

                        var image = canvas.toDataURL('image/png');
                        var resultimg = image;
                        var ImageSave = image.replace("data:image/png;base64,", "");
                        var image = JSON.stringify({ data: ImageSave, check: check });

                        console.log(check);

                        $.ajax({
                            type: "POST",
                            contentType: "application/json",
                            datatype: "json",
                            data: image,
                            url: "/Home/ImageValidationAPI/",
                            success: function (data) {
                                console.log(data);
                                if (data.Result == "Success") {
                                    $("#result_img").attr('src', resultimg);
                                    document.getElementById("form_submit").disabled = false;
                                }
                                else {

                                    console.log(data.Result);
                                    $("#errmsg").html(data.Result);
                                    $('#alert_popup').modal('show');
                                }
                                $('#snap_btn').button('reset');
                            }
                        });

                        snapshots.unshift(canvas);
                    }

                });


                $("#form_submit").click(function (e) {
                    e.preventDefault();
                    $('#form_submit').button('loading');
                    var Name = $("#name").val();
                    var gender = $("#gender").val();
                    var phone = $("#phonenumber").val();
                    var email = $("#email").val();
                    var image1 = $("#result_img").attr('src');

                    if (Name != "" && image1 != "") {
                        var img_64_array = image1.split(',');
                        var img_extension = img_64_array[0];
                        var ImageBase64 = image1.replace(img_extension + ",", "");
                        var endres = JSON.stringify({ data: ImageBase64, name: Name, gender: gender, phone: phone, email: email });

                        $.ajax({
                            type: "POST",
                            contentType: "application/json",
                            datatype: "json",
                            data: endres,
                            url: "/Home/FaceRegisterAPI/",
                            success: function (data) {
                                console.log(data.Result);
                                $("#errmsg").html(data.Result);
                                $('#alert_popup').modal('show');
                                $('#form_submit').button('reset');

                            }
                        });
                    }
                    else {
                        $("#errmsg").html("Please enter your name");
                        $('#alert_popup').modal('show');
                        $('#form_submit').button('reset');
                    }

                });

                $('#on_off_btn').click(function () {
                    if ($('#on_off_btn').prop("checked") == true) {
                        $('#video').hide();
                        $('#instruction').html("Please upload one of your smiling image");
                        $('#snap_btn').html("Submit");
                        $('#local_img').show();
                        $('#open_image_trigger').show();
                        $('#snap_btn').prop('disabled', true);
                    }
                    else {
                        $('#video').show();
                        $('#instruction').html("Please Smile and Take a Snap");
                        $('#snap_btn').html("Take Snapshot");
                        $('#local_img').hide();
                        $('#open_image_trigger').hide();
                        $('#snap_btn').prop('disabled', false);
                    }
                });



            });
            function imageopen() {
                $('#open_image_file').trigger('click');
            }

            var local_canvas, local_ctx;
            var img_src_64;
            local_canvas = document.getElementById("local_img");
            local_ctx = local_canvas.getContext('2d');
            var local_imageObj = new Image();

            local_imageObj.onload = function () {
                local_ctx.drawImage(local_imageObj, 0, 0, 300, 150);
            };
            local_imageObj.src = '/assets/images/empty.jpg';

            $(function () {
                $(":file").change(function () {
                    $('#snap_btn').prop('disabled', false);
                    if (this.files && this.files[0]) {
                        var reader = new FileReader();
                        reader.onload = imageIsLoaded;
                        reader.readAsDataURL(this.files[0]);
                    }
                });
            });
            function imageIsLoaded(e) {
                img_src_64 = e.target.result;
                var img_64_array = img_src_64.split(',');
                var img_extension = img_64_array[0];

                if (img_extension == "data:image/png;base64" || img_extension == "data:image/PNG;base64" || img_extension == "data:image/jpeg;base64" || img_extension == "data:image/JPEG;base64" || img_extension == "data:image/jpg;base64" || img_extension == "data:image/JPG;base64") {
                    local_imageObj.src = e.target.result;
                    $('#warn_msg').hide();
                }
                else {
                    $('#warn_msg').show();
                }
            };



