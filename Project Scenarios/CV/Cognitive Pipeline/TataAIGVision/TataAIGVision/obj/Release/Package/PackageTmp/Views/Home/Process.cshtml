            /*
            Carousel
            */
            $('#carousel-example').on('slide.bs.carousel', function (e) {
                var $e = $(e.relatedTarget);
                var idx = $e.index();
                var itemsPerSlide = 5;
                var totalItems = $('.carousel-item').length;
                if (idx >= totalItems - (itemsPerSlide - 1)) {
                    var it = itemsPerSlide - (totalItems - idx);
                    for (var i = 0; i < it; i++) {
                        // append slides to end
                        if (e.direction == "left") {
                            $('.carousel-item').eq(i).appendTo('.carousel-inner');
                        }
                        else {
                            $('.carousel-item').eq(0).appendTo('.carousel-inner');
                        }
                    }
                }
            });
            $(".cheque_img").click(function (e) {
                $('#process_err_msg').html("");
                var img_id = $(this).attr('id');
                $('.cheque_img').removeClass('cheque_img_border');
                $(this).addClass('cheque_img_border');
                var c = document.getElementById("myCanvas");
                var ctx = c.getContext("2d");
                var imageObj = new Image();
                ctx.canvas.width = window.innerWidth;
                ctx.canvas.height = window.innerHeight;
                imageObj.onload = function () {
                    ctx.drawImage(imageObj, 0, 0, 1500, 700);
                    $('#preview_img').attr('src', c.toDataURL());
                    $('#norming_img').attr('src', c.toDataURL());
                    $('#forming_img').attr('src', c.toDataURL());
                    $('#cracking_img').attr('src', c.toDataURL());
                    $('#content_building_img').attr('src', c.toDataURL());
                    $('#rule_engine_internal_img').attr('src', c.toDataURL());
                    $('#rule_engine_external_img').attr('src', c.toDataURL());
                };
                imageObj.src = $('#' + img_id).attr('src');
            });

            $("#process_btn").click(function (e) {
                $('#output_sec').hide();
                $('#Chequeimg').hide();
                $('#DemandDraftimg').hide();
                $('#PANCardimg').hide();
                $('#result_json').html("");
                $('#rule_internal_customer_name').hide();
                $('#rule_internal_bank_name').hide();
                $('#rule_internal_date').hide();
                $('#rule_internal_amount_in_digits').hide();
                $('#rule_internal_amount_in_words').hide();
                $('#rule_internal_all').hide();
                $('#re_customername').removeClass('textbox_red');
                $('#re_customername').removeClass('textbox_green');
                $('#re_bankname').removeClass('textbox_red');
                $('#re_bankname').removeClass('textbox_green');
                $('#re_date').removeClass('textbox_red');
                $('#re_date').removeClass('textbox_green');
                $('#re_amountindigits').removeClass('textbox_red');
                $('#re_amountindigits').removeClass('textbox_green');
                $('#re_customername').val("");
                $('#re_bankname').val("");
                $('#re_date').val("");
                $('#re_amountindigits').val("");
               
                var preview_img_src = $('#preview_img').attr('src');
                var img_64_array = preview_img_src.split(',');
                var preview_img_src_final = img_64_array[1];
                if (preview_img_src == "/TATAAIGassets/img/preview.png") {
                    $('#process_err_msg').html("Please select any one image");
                    $('#process_err_msg').css('color', 'red');
                }
                else {
                    var vision_data = JSON.stringify({
                        data: preview_img_src_final
                    });
                    $('#process_err_msg').html("Please Wait..");
                    $("#process_err_msg").css("color", "black");
                    var get_data = $(this).attr('loading-text');
                    $("#process_btn").html(get_data);
                    $('#process_btn').button('loading');
                    $("#process_btn").attr("disabled", true);
                    var Url = "/Home/TataAIGVision/";
                    $.ajax({
                        type: "POST",
                        contentType: "application/json",
                        datatype: "json",
                        data: vision_data,
                        url: Url,
                        success: function (response) {
                            console.log(response);
                            if (response.StatusCode == "S") {
                                $("#process_btn").attr("disabled", false);
                                $('#output_sec').show();
                                $('#process_err_msg').html("");
                                $('#cb_customername').val("");
                                $('#cb_bankname').val("");
                                $('#cb_date').val("");
                                $('#cb_amountinwords').val("");
                                $('#cb_amountindigits').val("");
                                var onbulbsrc = "/TATAAIGassets/img/pic_bulbon.gif";
                                var offbulbsrc = "/TATAAIGassets/img/pic_bulboff.gif";
                                if (response.Type == "Cheque") {
                                    $('#Chequeimg').attr('src', onbulbsrc);
                                    $('#DemandDraftimg').attr('src', offbulbsrc);
                                    $('#PANCardimg').attr('src', offbulbsrc);
                                    $('#Chequeimg').show();
                                    $('#DemandDraftimg').show();
                                    $('#PANCardimg').show();
                                }
                                else if (response.Type == "Demand Draft") {
                                    $('#Chequeimg').attr('src', offbulbsrc);
                                    $('#DemandDraftimg').attr('src', onbulbsrc);
                                    $('#PANCardimg').attr('src', offbulbsrc);
                                    $('#Chequeimg').show();
                                    $('#DemandDraftimg').show();
                                    $('#PANCardimg').show();
                                }
                                else if (response.Type == "PAN") {
                                    $('#Chequeimg').attr('src', offbulbsrc);
                                    $('#DemandDraftimg').attr('src', offbulbsrc);
                                    $('#PANCardimg').attr('src', onbulbsrc);
                                    $('#Chequeimg').show();
                                    $('#DemandDraftimg').show();
                                    $('#PANCardimg').show();
                                }


                                var result_json = JSON.stringify(response);
                                $('#result_json').html(result_json);


                                if (response.CustomerName != null && response.BankName != null && response.Date != null && response.AmountInWords != null && response.AmountInDigits != null) {
                                    $('#rule_internal_all').show();
                                }


                                if (response.CustomerName != null) {
                                    $('#cb_customername').val(response.CustomerName);
                                }
                                else {
                                    $('#rule_internal_customer_name').show();
                                }
                                if (response.BankName != null) {
                                    $('#cb_bankname').val(response.BankName);
                                }
                                else {
                                    $('#rule_internal_bank_name').show();
                                }
                                if (response.Date != null) {
                                    $('#cb_date').val(response.Date);
                                }
                                else {
                                    $('#rule_internal_date').show();
                                }
                                if (response.AmountInWords != null) {
                                    $('#cb_amountinwords').val(response.AmountInWords);
                                }
                                else {
                                    $('#rule_internal_amount_in_words').show();
                                }
                                if (response.AmountInDigits != null) {
                                    $('#cb_amountindigits').val(response.AmountInDigits);
                                }
                                else {
                                    $('#rule_internal_amount_in_digits').show();
                                }
                            }
                            else {
                                $("#process_btn").attr("disabled", false);
                                $('#process_err_msg').html("");
                                alert(response.Message);
                            }
                           


                        },
                        error: function (response) {
                            $("#process_btn").attr("disabled", false);
                            $('#process_err_msg').html("");
                          alert("Something went wrong. Please Try again")
                        }
                    });
         
                }
            });

