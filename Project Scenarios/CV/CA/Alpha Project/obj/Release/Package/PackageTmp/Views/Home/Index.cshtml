    <script>
        $(document).ready(function () {
            $('#process_btn').click(function () {
                var table = $('#data_table').DataTable();



                table.clear()
                table.draw();
                if (window.FormData !== undefined) {
                    var fileUpload = $("#FileUpload").get(0);
                    var files = fileUpload.files;

                    // Create FormData object
                    var fileData = new FormData();

                    // Looping over all files and add it to FormData object
                    for (var i = 0; i < files.length; i++) {
                        fileData.append(files[i].name, files[i]);
                    }

                    // Adding one more key to FormData object
                    fileData.append('username', 'Manas');
                    //fileData.append('fileextension', file_extension);
                    //debugger;
                    $('#json_result').html("");
                    var get_data = $('#process_btn').attr('data-loading-text');
                    $('#process_btn').html(get_data);
                    //$('#process_btn').button('loading');
                    $('#process_btn').attr("disabled", true);
                    $.ajax({
                        url: '/Home/UploadFiles',
                        type: "POST",
                        contentType: false, // Not to set any content header
                        processData: false, // Not to process data
                        data: fileData,
                        success: function (result) {

                            if (result.StatusCode == 'S') {
                                console.log(result.FullJSON);
                                for (var i = 0; i < result.FullJSON.length; i++) {
                                    //debugger;
                                    $("#data_table").dataTable().fnDestroy();

                                    $('#data_table tbody').append('<tr><td>' + result.FullJSON[i][0] + '</td><td>' + result.FullJSON[i][1] + '</td><td>' + result.FullJSON[i][2] + '</td><td>' + result.FullJSON[i][3] + '</td><td>' + result.FullJSON[i][4] + '</td><td>' + result.FullJSON[i][5] + '</td></tr>');
                                    var table = $('#data_table').DataTable();
                                }
                                //$('#json_result').html(syntaxHighlight(JSON.stringify(result.FullJSON, undefined, 4)));
                                $('#process_btn').html("Process");
                                $('#process_btn').attr("disabled", false);
                            }
                            else {
                                $('#err_msg').html(result.Message);
                            }
                        },
                        error: function (err) {

                            $('#process_btn').html("Process");
                            $('#process_btn').attr("disabled", false);
                            alert(err.statusText);
                        }
                    });
                } else {

                    $('#process_btn').html("Process");
                    $('#process_btn').attr("disabled", false);
                    alert("FormData is not supported.");
                }
            });
        });


        function syntaxHighlight(json) {
            json = json.replace(/&/g, '&amp;').replace(/</g, '&lt;').replace(/>/g, '&gt;');
            return json.replace(/("(\\u[a-zA-Z0-9]{4}|\\[^u]|[^\\"])*"(\s*:)?|\b(true|false|null)\b|-?\d+(?:\.\d*)?(?:[eE][+\-]?\d+)?)/g, function (match) {
                var cls = 'number';
                if (/^"/.test(match)) {
                    if (/:$/.test(match)) {
                        cls = 'key';
                    } else {
                        cls = 'string';
                    }
                } else if (/true|false/.test(match)) {
                    cls = 'boolean';
                } else if (/null/.test(match)) {
                    cls = 'null';
                }
                return '<span class="' + cls + '">' + match + '</span>';
            });
        }

    </script>
